  <link name="base">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="3."/>
      <inertia ixx="1." ixy="0.0" ixz="0.0" iyy="1." iyz="0.0" izz="1."/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="/stl/base.stl"/>
      </geometry>
    </visual>
        <collision>
      <geometry>
        <mesh filename="/stl/base.stl"/>
      </geometry>
    </collision>
  </link>




